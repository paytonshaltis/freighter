{
  "compilerOptions": {
    "target": "es6",
    // "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist/js-output",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
